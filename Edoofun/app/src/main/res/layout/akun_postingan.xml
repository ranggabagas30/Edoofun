        tools:layout_editor_absoluteX="132dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="#5AD7FD"
        android:textSize="20sp"
